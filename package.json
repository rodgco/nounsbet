{
	"name": "nounsbet",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"test": "playwright test",
		"vitest": "vitest",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"svg": "$npm_execpath run svg:optimize && $npm_execpath run svg:to-path",
		"svg:optimize": "svgo -f ./src/icons -o ./src/icons",
		"svg:to-path": "svgtofont --sources ./src/icons --output ./static/fonts"
	},
	"svgtofont": {
		"fontName": "bet",
		"website": false,
		"outSVGReact": false,
		"css": {
			"include": "\\.(css)$",
			"output": "./static",
			"fileName": "bet-icon",
			"cssPath": "./fonts/"
		}
	},
	"devDependencies": {
		"@nouns/assets": "^0.2.0",
		"@nouns/contracts": "^0.1.3",
		"@nouns/sdk": "^0.1.6",
		"@playwright/test": "^1.20.1",
		"@supabase/supabase-js": "^1.33.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@testing-library/svelte": "^3.1.0",
		"@types/jest": "^27.4.1",
		"@types/qrcode": "^1.4.2",
		"@typescript-eslint/eslint-plugin": "^5.17.0",
		"@typescript-eslint/parser": "^5.17.0",
		"@vitest/ui": "latest",
		"dotenv": "^16.0.0",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"ethers": "^5.6.2",
		"jsdom": "latest",
		"prettier": "^2.6.1",
		"prettier-plugin-svelte": "^2.6.0",
		"qrcode": "^1.5.0",
		"svelte": "^3.46.6",
		"svelte-check": "^2.4.6",
		"svelte-preprocess": "^4.10.4",
		"svgo": "^2.8.0",
		"svgtofont": "^3.17.6",
		"tslib": "^2.3.1",
		"typescript": "^4.6.3",
		"vitest": "^0.8.2",
		"vitest-svelte-kit": "^0.0.6"
	},
	"type": "module"
}
